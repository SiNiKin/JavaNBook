
public class P_M_13_1 {

	public static void main(String[] args) {
		
		int y = 0;
		while (y < 10) {
			int x = 1;
			while (x < 10) {
				if (y == 0) {
					System.out.print(x + "ë‹¨ \t");
				}else {
					System.out.print(x + "*" + y + "=" + x*y + "\t");
				}
				x++;
			}
			System.out.println("");
			y++;
		}
		
		for (int j = 1; j < 10; j ++) {
			for (int i = 1; i < 10; i ++) {
				System.out.println(i + " x " + j + " = " + (i*j));
			}
			System.out.print("");
		}
	}

}
