
public class P_M_13_1 {

	public static void main(String[] args) {
		
		int y = 0;
		while (y < 10) {
			int x = 2;
			while (x < 10) {
				if (y == 0) {
					System.out.print(x + "ë‹¨ \t");
				}else {
					System.out.print(x + "*" + y + "=" + x*y + "\t");
				}
				x++;
			}
			System.out.println("");
			y++;
		}
	}

}
